.datapage_print
  overflow hidden
  height 1px
  width 750px
  margin auto
  #DataSummary
    > .bars
      display none !important
    > .inner
      padding 20px 0
      > .copy
        padding 0 0 10px 0 !important
      > .stats
        height auto
        > .stat
          padding 10px 30px
          > .value
            font h1b
            height 30px
          > .copy
            font c1ss


  .section_chart
    margin-top 20px
    > .chart.left
      float left
      width 520px
      > #HousingGap
        padding 10px
        border none
        .title
          padding 0 0 10px 0
        .stat
          clear both
          display block
          position static
          float none
          padding 0 0 10px 0

  .section_demand
    position relative
    float right
    background-color lightwhite
    width 210px
    border-radius 6px
    padding 10px
    p:nth-child(1)
      font h1
      margin 10px 0
    P > span > span
      color red
    .pointer
      position absolute
      width 3px
      height 40px
      background-color black
      left 30px
      bottom -20px

  .section_charts
    display flex
    .chart:nth-child(1)
      border-right 1px solid grey
    .chart:nth-child(2)
      padding-left 10px
      margin-right 5px
      padding-right 5px
      border-right 1px solid grey
    .chart:nth-child(3)
      margin-left 10px
    .chart
      width 33%
      > .copys
        > .copy:nth-child(1)
          font c1b
        > .copy:nth-child(2)
          font c1ss
          color grey
          height 120px
      > .title
        font c1sb
        text-transform uppercase
      #NationalStats
        width 100%


  > .logos
    width 750px
    height 30px
    margin 20px auto 0 auto
    text-align right
    > img
      height 50px
  > .source
    text-align right
    color grey
    padding 6px
    font c1s

